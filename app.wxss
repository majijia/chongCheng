/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/*弹性*/
.metSex{
  display:flex;
  justify-content: space-around;
}
/*大按钮*/
.loginBtn{
  width:540rpx;
  height:88rpx;
  background:#ff8833;
  border-radius:44rpx;
  /* margin-top:50%; */
  color:rgba(255,255,255,1);
  font-size:34rpx;
  line-height: 88rpx;
  text-align: center;
  margin:50% auto 0;
}
/*箭头*/
.jianTwo{
  width:16rpx;
  height:28rpx;
}
/*radio大小*/
.vidio{
  transform:scale(0.7);
}
/*一键到顶*/
.goTop{
  position: fixed;
  top:90%;
  left:85%;
  width:88rpx;
  height:88rpx;
}
/*固定按钮*/
.vidPlayTs{
    background: #fff;
    position: fixed;
    bottom: 0;
    display: flex;
    width: 100%;
    justify-content: space-around;
    text-align: center;
}
.Tones{
    margin:20rpx 0;
}
.Tones view{
    color:#999;
    font-size: 22rpx;
}
.Tones image:nth-child(1){
    width:38rpx;
    height: 38rpx;
}