<view>
    <swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{1000}}">
        <block wx:for="{{imgUrls}}">
            <swiper-item>
            <image src="{{item}}" mode="scaleToFill" class="bannerImg" width="100%" height="320"/>
            </swiper-item>
        </block>
    </swiper>
</view>